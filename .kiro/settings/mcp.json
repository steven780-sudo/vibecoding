{
  "mcpServers": {
    "context7": {
      "command": "npx",
      "args": [
        "-y",
        "@upstash/context7-mcp",
        "--api-key",
        "ctx7sk-d4305efc-9b48-4f19-8f76-9e4e264975b2"
      ],
      "env": {},
      "disabled": false,
      "autoApprove": [
        "search_documentation",
        "get_documentation"
      ]
    },
    "playwright": {
      "command": "npx",
      "args": [
        "-y",
        "@executeautomation/playwright-mcp-server"
      ],
      "env": {},
      "disabled": false,
      "autoApprove": [
        "get_browser_state",
        "get_console_logs",
        "playwright_navigate"
      ]
    },
    "chrome-devtools": {
      "command": "npx",
      "args": [
        "-y",
        "chrome-devtools-mcp@latest"
      ],
      "env": {},
      "disabled": false,
      "autoApprove": [
        "get_console_logs",
        "get_network_logs",
        "get_page_info",
        "navigate_page",
        "take_screenshot",
        "take_snapshot",
        "list_console_messages",
        "list_network_requests",
        "new_page",
        "fill",
        "click",
        "list_pages"
      ]
    },
    "memory": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-memory"
      ],
      "env": {},
      "disabled": false,
      "autoApprove": [
        "create_entities",
        "create_relations",
        "search_nodes"
      ]
    },
    "sequential-thinking": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-sequential-thinking"
      ],
      "env": {},
      "disabled": false,
      "autoApprove": [
        "sequentialthinking"
      ]
    },
    "filesystem": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-filesystem",
        "."
      ],
      "env": {},
      "disabled": false,
      "autoApprove": [
        "read_file",
        "list_directory",
        "read_media_file"
      ]
